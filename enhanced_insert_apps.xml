<?xml version="1.0"?>
<root>
	<include once="true" path="vk_apps.xml" />
	<item>
		<name>Insert + Enter to open Launchpad</name>
		<identifier>private.InsertEnterLaunchpad</identifier>
		<autogen>
			__KeyToKey__
			KeyCode::RETURN, ModifierFlag::EXTRA6 | ModifierFlag::NONE,
			KeyCode::LAUNCHPAD
		</autogen>
	</item>
